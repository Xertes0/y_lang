
@do_nothing
_begin
    :void _ret
_end

@do_almost_smth
[ :i32 $b
  :i32 $a ]
_begin
    $b $a _add !
    :void _ret
_end

@do_smth :i32
[ :i32 $b
  :i32 $a ]
_begin
    $b $a _add _ret
_end

@std_put_s
[ :i8* ]

@std_put_i32
[ :i32 ]

@main :i32
_begin
    @do_nothing !
    2 3 @do_almost_smth !

    "Hello world: " @std_put_s !
    1234 @std_put_i32 !
    "\n" @std_put_s !

    0 0 @do_smth _ret
_end

